	Journal = {Journal of Political Economy},
	Number = {4},
	Pages = {898-927},
	Publisher = {University of Chicago Press},
	Title = {The Substantial Bias from Ignoring General Equilibrium Effects in Estimating Excess Burden, and a Practical Solution},
	Volume = {111},
	Year = {2003},
